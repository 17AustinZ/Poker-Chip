FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/ActionButton.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/Bohr.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/LGPlusButtonsView.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/Mixpanel.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/SimpleAlert.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "ActionButton" -framework "Bohr" -framework "LGPlusButtonsView" -framework "Mixpanel" -framework "SimpleAlert"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-ChipManager
PODS_ROOT = ${SRCROOT}/Pods